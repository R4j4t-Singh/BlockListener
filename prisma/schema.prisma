// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ContractAddresses {
  id             Int        @id @default(autoincrement())
  name          String?
  address        String     @unique
  creationDate   String     @default("NA")
  tokenValue          String     @default("{}")
  InteractedAddresses InteractedAddresses[]
  network String @default("ETHEREUM_MAINNET")
}

model InteractedAddresses {
  id             Int       @id @default(autoincrement())
  address        String    
  timestamp      Int
  contractAddressId Int 
  ContractAddresses ContractAddresses @relation(fields: [contractAddressId], references: [id])
}

model Transactions {
  id              Int        @id @default(autoincrement())
  transactionHash String    
  blockNumber     Int
  timeStamp       Int
  from            String
  to              String
  network String @default("ETHEREUM_MAINNET")
}